{
  "hosting": {
    "public": "hosting",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "downloads/**",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/octet-stream"
          },
          {
            "key": "Content-Disposition",
            "value": "attachment"
          }
        ]
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
